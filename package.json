{
    "name": "@recuperateur/async-busboy",
    "version": "1.2.1",
    "description": "Promise based multipart form parser",
    "main": "./lib/cjs/index.js",
    "module": "./lib/esm/index.js",
    "types": "./lib/types/index.d.ts",
    "exports": {
        ".": {
            "require": "./lib/cjs/index.js",
            "import": "./lib/esm/index.js"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Recuperateur/async-busboy"
    },
    "scripts": {
        "prepublishOnly": "npm run build:clean && npm run build",
        "build": "npm run build:module && npm run build:commonjs",
        "build:clean": "rm -r lib/*",
        "build:module": "tsc",
        "build:commonjs": "tsc --project tsconfig.commonjs.json",
        "test": "NODE_ENV=test mocha specs.js --harmony --reporter spec --bail",
        "cover": "NODE_ENV=test istanbul cover -x specs.js _mocha specs.js -- --reporter spec --bail",
        "lint": "eslint \"{src,apps,libs,test}/**/*.{tsx,ts}\" --fix",
        "lint:test": "eslint \"{src,apps,libs,test}/**/*.{tsx,ts}\""
    },
    "keywords": [
        "koa",
        "multipart",
        "form",
        "file-upload"
    ],
    "author": "Currently maintain by Inist-CRNS",
    "contributors": [
        {
            "name": "Current Maintainer: AlasDiablo",
            "url": "https://github.com/AlasDiablo"
        },
        {
            "name": "Creator of the fork for busboy 1.4.0: alqu",
            "url": "https://github.com/alqu"
        },
        {
            "name": "Original Author: Emanuel Chappat",
            "email": "emmanuel.chappat@gmail.com",
            "url": "https://github.com/m4nuC/async-busboy"
        }
    ],
    "license": "MIT",
    "dependencies": {
        "busboy": "^1.6.0"
    },
    "devDependencies": {
        "@draconides/eslint-config-ts": "^1.1.1",
        "@types/busboy": "^1.5.0",
        "@typescript-eslint/eslint-plugin": "^6.7.0",
        "@typescript-eslint/parser": "^6.7.0",
        "eslint": "^8.49.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-prettier": "^5.0.0",
        "expect": "^27.5.1",
        "formstream": "^1.3.1",
        "istanbul": "^0.4.5",
        "mocha": "^10.2.0",
        "prettier": "^3.0.3",
        "typescript": "^5.2.2"
    }
}
